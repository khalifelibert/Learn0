<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Maui0.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Maui0"
    xmlns:view="clr-namespace:Maui0.ViewPage"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>

        <Tab>
            <ShellContent
                Title="KIK"
                ContentTemplate="{DataTemplate local:Page0}"
                 Route="Page0"/>

        </Tab>

        <Tab>
            <ShellContent
                Title="KIK"
                ContentTemplate="{DataTemplate view:Page1}"
                 Route="Page1"/>

        </Tab>

    </TabBar>
</Shell>
